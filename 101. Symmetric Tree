/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public boolean isSymmetric(TreeNode root) {
        Queue<TreeNode> que = new LinkedList<>();
        que.add(root);
        que.add(root);

        while(!que.isEmpty()){
            TreeNode p = que.poll();
            TreeNode q = que.poll();

            if(p==null && q==null){
                continue;
            }
            else if(p==null || q==null){
                return false;
            }
            else if(p.val != q.val){
                return false;
            }
            que.add(p.left);
            que.add(q.right);
            que.add(p.right);
            que.add(q.left);
        }
        return true;
    }
}
